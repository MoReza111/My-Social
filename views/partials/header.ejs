<div class="header">
    <div class="header-logo">
        <a href="/">My-Social</a>
    </div>
    <form action="" method="post">
        <input class="search-input" type="text" placeholder="Search" />
        <button type="submit"><i class="fas fa-search"></i></button>
    </form>
    <nav>
        <ul>
            <li><a href="/"><i class="fas fa-home"></i></a></li>
            <li><a href="#"><i class="far fa-heart"></i></a></li>
            <% if(user){ %>
                <li><a href="/users/profile"><img src="/images/<%= user.profile_image %>" alt="profile-pic" class="nav-img" /></a></li>
            <% }else{  %>
                <li><a href="/users/register"><img src="/images/undefind.png" alt="profile-pic" class="nav-img" /></a></li>
            <% } %>
        </ul>
    </nav>
</div>